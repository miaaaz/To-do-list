<%- include('header') %> 
<body>
    <div class="box" id="heading">
        <h1><%= kindOfDay %> </h1>
    </div>
    
    <div class="box">
        <% for (let i = 0; i < newListItems.length; i++) { %>
        <div class="item">
            <input type="checkbox">
            <p><%= newListItems[i] %> </p>
        </div>        
        <% } %>

        <form class="item" action="/" method="post">
            <input type="text" placeholder="New Item" name="newItem" autocomplete="off">
            <button type="submit" name="button">+</button>
        </form>    
            
    </div>


    
</body>
</html>